    "extends": [
      "@wordpress/stylelint-config/scss"
    ],
    "rules": {
      "no-descending-specificity": null,
      "selector-class-pattern": null,
      "unit-allowed-list": [
        "em",
        "px",
        "%",
        "s",
        "rem",
        "vw",
        "vh",
        "ms",
        "fr"
      ]
    }
